<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exercise">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Exercise">
        <activity android:name=".MyDiaryModify"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar"/>
        <activity android:name=".MyDiaryWrite"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar"/>
        <activity android:name=".MyDiary"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar"/>
        <activity
            android:name=".PillModify"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".PillWrite"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".ManagePill"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".FoodModify"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".FoodWrite"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".ManageFood"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".FoodMainActivity"
            android:exported="true"
            android:theme="@style/Theme.Exercise.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Exercise.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>